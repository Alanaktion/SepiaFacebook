{
	"name": "Sepia Facebook",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Make Facebook look a bit older",
	"content_scripts": [{
		"matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"],
		"css": ["sepia.css"],
	}],
	"permissions": [
		"tabs",
		"http://www.facebook.com/*",
		"https://www.facebook.com/*"
	]
}